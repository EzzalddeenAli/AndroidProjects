package com.longluo.whileloop;

public class WhileLoop {

	private static void main(String args[]) {
		System.out.println("begin...");
		boolean bLoop = false;
		boolean bEnd = false;
		boolean bOn = bLoop || bEnd;
		
		while (true) {
			if (bOn) {
				try {
					System.out.println("bLoop=" + bLoop + "bOn = " + bOn);
				} catch() {
					
				}
			}
		}
	}
}
